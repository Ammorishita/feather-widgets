@model Telerik.Sitefinity.Frontend.Forms.Mvc.Models.Fields.PageBreak.PageBreakViewModel

@using Telerik.Sitefinity.Frontend.Mvc.Helpers;
@using Telerik.Sitefinity.Modules.Pages;

@Html.Script(ScriptRef.JQuery, "top", false)

<div class="@Model.CssClass">
    @if (Model.AllowGoBack)
    {
        <span class="prev">@Html.Raw(Model.PreviousStepText) </span>
    }
    <span class="next">@Html.Raw(Model.NextStepText) </span>
</div>
@Html.Script(Url.WidgetContent("Mvc/Scripts/Form/form-multi-step.js"), "bottom", false)

