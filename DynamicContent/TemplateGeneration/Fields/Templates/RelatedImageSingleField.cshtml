@model Telerik.Sitefinity.DynamicModules.Builder.Model.DynamicModuleField

@@*Start @Model.Name field*@@
@@{{
    var item = Model.Item.RelatedItems("@Model.Name").GetSingle<RelatedImageViewModel>();
    if(item != null)
    {
        <img src='@@item.MediaUrl' alt='@@item.AlternativeText' title='@@item.Title' />
    }
}}
@@*End @Model.Name field*@@