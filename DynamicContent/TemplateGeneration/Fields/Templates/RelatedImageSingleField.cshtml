@model Telerik.Sitefinity.DynamicModules.Builder.Model.DynamicModuleField

@@*Start @Model.Name field*@@
@@{{
    var item = Model.Item.RelatedItem("@Model.Name");
    if(item != null)
    {
        <img src='@@item.Fields.MediaUrl' alt='@@item.Fields.AlternativeText' title='@@item.Fields.Title' />
    }
}}
@@*End @Model.Name field*@@