@model Telerik.Sitefinity.DynamicModules.Builder.Model.DynamicModuleField

@using Telerik.Sitefinity.Model;
@using Telerik.Sitefinity.Libraries.Model;

@@*Start @Model.Name field*@@
<ul>
@@foreach (IDynamicFieldsContainer relatedItem in ((object)Model.Item).GetRelatedItems("@Model.Name").ToList<IDataItem>())
{
    <li class="media">
        <a href='@@(((MediaContent)relatedItem).MediaUrl)'>@@relatedItem.GetValue("Title")</a>
    </li>   
}
</ul>
@@*End @Model.Name field*@@