<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>

<div data-sf-field="<#= fieldData.Name #>" data-sf-ftype="DateTime">
    <strong> <#= fieldData.Title #> :</strong>

	@if(Model.Item.<#= fieldData.Name #>!=null)
	{
    <span>@(((DateTime)Model.Item.<#= fieldData.Name #>).ToSitefinityUITime().ToString("MMM d, yyyy, HH:mm tt"))</span>
	} 
</div>