@model Telerik.Sitefinity.DynamicModules.Builder.Model.DynamicModuleField

@using Telerik.Sitefinity.RelatedData;

<div>
    <strong> @Model.FrontendWidgetLabel :</strong>

    <ul>

    @@foreach (IDynamicFieldsContainer relatedItem in ((object)Model.Item).GetRelatedItems(@Model.Name).ToList<IDataItem>())
    {
        <li><a href='@@Telerik.Sitefinity.RelatedData.RelatedDataExtensions.GetDefaultUrl(relatedItem)'>@@relatedItem.GetValue(@(RelatedDataHelper.GetRelatedTypeIdentifierField(Model.RelatedDataType)))</a></li>   
    }
    </ul>
</div>